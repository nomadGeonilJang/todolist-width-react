er-radius: 16px;\n  box-shadow: 0 0 8px rgba(0, 0, 0, 0.04);\n\n  margin: 0 auto;\n  margin-top: 96px;\n  margin-bottom: 32px;\n\n  display: flex;\n  flex-direction: column;\n`;\n\nfunction TodoTemplate({ children }: any) {\n  return <TodoTemplateBlock>{children}</TodoTemplateBlock>;\n}\n\nexport default TodoTemplate;\n","/Users/jang-geon-il/Project/todolist-with-react/src/components/TodoCreate.tsx",["91","92"],"import React, { useEffect, useState } from \"react\";\nimport { MdAdd } from \"react-icons/md\";\nimport { useDispatch } from \"react-redux\";\nimport useClearStage from \"../hooks/useClearStage\";\nimport useToggleUpdate from \"../hooks/useToggleUpdate\";\nimport { createTodo, updateTodo } from \"../store/reducers/todo\";\n\nimport {\n  CircleButton,\n  Input,\n  InserForm,\n  InsertFromPositioner,\n} from \"./TodoCreate.style\";\n\nfunction TodoCreate() {\n  const dispatch = useDispatch();\n  const stage = useToggleUpdate();\n  const clearStage = useClearStage();\n\n  const [open, setOpen] = useState(false);\n  const [value, setValue] = useState(\"\");\n\n  const onToggle = () => {\n    clearStage();\n    setOpen(!open);\n  };\n\n  const onChangeText = (e: React.ChangeEvent<HTMLInputElement>) =>\n    setValue(e.target.value);\n\n  const onSubmit = (e: React.FormEvent) => {\n    e.preventDefault();\n    setValue(\"\");\n    if (stage) {\n      dispatch(updateTodo({ text: value, id: stage.id }));\n    } else {\n      dispatch(createTodo(value));\n    }\n    setOpen(false);\n  };\n\n  useEffect(() => {\n    if (stage && stage.id) {\n      setOpen(true);\n      setValue(stage.text);\n    } else {\n      setValue(\"\");\n    }\n  }, [stage && stage.id]);\n\n  return (\n    <>\n      {open && (\n        <InsertFromPositioner>\n          <InserForm onSubmit={onSubmit}>\n            <Input\n              value={value}\n              onChange={onChangeText}\n              autoFocus\n              placeholder=\"할 일을 입력 후, Enter를 누르세요\"\n            />\n          </InserForm>\n        </InsertFromPositioner>\n      )}\n      <CircleButton onClick={onToggle} open={open}>\n        <MdAdd />\n      </CircleButton>\n    </>\n  );\n}\n\nexport default React.memo(TodoCreate);\n","/Users/jang-geon-il/Project/todolist-with-react/src/components/TodoCreate.style.ts",[],"/Users/jang-geon-il/Project/todolist-with-react/src/hooks/useToggleUpdate.ts",[],"/Users/jang-geon-il/Project/todolist-with-react/src/hooks/useClearStage.ts",["93"],"import { useCallback } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport { toggleUpdate } from \"../store/reducers/todo\";\nexport default function useClearStage() {\n  const dispatch = useDispatch();\n\n  const clearStage = useCallback(() => {\n    dispatch(toggleUpdate(null));\n  }, []);\n\n  return clearStage;\n}\n","/Users/jang-geon-il/Project/todolist-with-react/src/utils/storage.ts",["94"],{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","severity":1,"message":"100","line":14,"column":6,"nodeType":"101","endLine":14,"endColumn":8,"suggestions":"102"},{"ruleId":"103","severity":1,"message":"104","line":4,"column":10,"nodeType":"105","messageId":"106","endLine":4,"endColumn":22},{"ruleId":"103","severity":1,"message":"107","line":1,"column":17,"nodeType":"105","messageId":"106","endLine":1,"endColumn":26},{"ruleId":"99","severity":1,"message":"108","line":49,"column":6,"nodeType":"101","endLine":49,"endColumn":25,"suggestions":"109"},{"ruleId":"99","severity":1,"message":"110","line":49,"column":7,"nodeType":"111","endLine":49,"endColumn":24},{"ruleId":"99","severity":1,"message":"112","line":9,"column":6,"nodeType":"101","endLine":9,"endColumn":8,"suggestions":"113"},{"ruleId":"114","severity":1,"message":"115","line":3,"column":1,"nodeType":"116","endLine":10,"endColumn":3},"no-native-reassign",["117"],"no-negated-in-lhs",["118"],"react-hooks/exhaustive-deps","React Hook useEffect ha